<decorators defaultdir="/WEB-INF/templates">
	
	<excludes>
        <pattern>/css/*</pattern>
        <pattern>/js/*</pattern>
    </excludes>
	
	<decorator name="print" page="print.jsp">
	    <pattern>/*/print/*</pattern>
	</decorator>
	
	<decorator name="default" page="shell.jsp">
	    <pattern>/*</pattern>
	</decorator>

</decorators>